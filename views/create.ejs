        <form style="margin-bottom:2rem; margin-left: 3rem;" action="/create" method="post">
            <h1>Question Paper Genrator</h1>
            <br>

            <div><label class = "form-label" for="ExamName">Exam Name</label>
                <select class="form-select" aria-label="Default select example" type = "text" name="exam">
                    <option selected>Open this select menu</option>
                    <option>Semester End Exam</option>
                    <option>Mid Semester Exam I</option>
                    <option>Mid Semester Exam  II</option>
                    <option>Mid Semester Exam  III</option>
                    <option>Learning Assesment I</option>
                    <option>Learning Assesment II</option>
                    <option>Improvment Exam</option>
                </select>
            </div>
            <br>
            <div> <label class = "form-label" for="Branch"> Branch :</label>

                <select class="form-select" aria-label="Default select example" type = "text" name="branch">
                    <option selected>Open this select menu</option>
                    <option>CSE</option>
                    <option>ISE</option>
                    <option>ECE</option>
                    <option>EEE</option>
                    <option>Mech</option>
                    <option>Aero</option>
                    <option>CIVIL</option>
                    <option>MCA</option>
                </select>
            </div>
            <br>
            <div class="mb-3">
                <label for="exampleInputPassword1" class="form-label" >Semester</label>
                <input class="form-control" id="exampleInputPassword1" type="text" name="sem">
              </div>
            
              <div class="mb-3">
                <label for="exampleInputPassword1" class="form-label" >Subject</label>
                <input class="form-control" id="exampleInputPassword1" type="text" name="sub">
              </div>
            <br>
            <div class="mb-3">
                <label for="exampleInputPassword1" class="form-label" >Subject Code</label>
                <input  class="form-control" id="exampleInputPassword1" type="text" name="subc">
              </div>
            <br>
         
            <p><label class = "form-label" for="section" >Questions have sections?</label>
                <select id="section" >
                    <option>yes</option>
                    <option>no</option>

                </select>
            </p>

            <br>
            <div class="mb-3">
                <label for="exampleInputPassword1" class="form-label">Instructions</label>
                <input  class="form-control" id="exampleInputPassword1" type="text" name="inst">
              </div>
            <br>

            <div class="mb-3">
                <label for="exampleInputPassword1" class="form-label" >Faculty</label>
                <input  class="form-control" id="exampleInputPassword1" type="text" name="fac">
              </div>
            <br>
            <p>
                <div class="mb-3">
                    <label for="exampleInputPassword1" class="form-label" >Time Duratiom</label>
                    <input  class="form-control" id="exampleInputPassword1" type="text" name="time">
                  </div>
            </p>
            <br>
            <div class="mb-3">
                <label for="exampleInputPassword1" class="form-label">Date</label>
                <input type="date" class="form-control" id="exampleInputPassword1"  name="date" >
              </div>
            <br>
            <div class="mb-3">
                <label for="exampleInputPassword1" class="form-label">Max Marks</label>
                <input type="number" class="form-control" id="exampleInputPassword1" name="marks">
              </div>
            <br>
            <p> <label for="status"> Status :</label>

                <select id="status">
                    <option>Active</option>
                    <option>Paused</option>
                    <option>Completed</option>
                    <option>Dropped</option>
                </select>
            </p>
            <br>

            <p>
                <button type="submit" class="btn btn-primary">Submit</button>
            </p>
            <br>
        </form>
   
